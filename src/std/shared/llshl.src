	assume	adl=1

	section	.text
	public	__llshl
__llshl:
; Suboptimal for large shift amounts
	push	af
	push	iy
	ld	iy, 0
	add	iy, sp
	ld	a, b
	ld	b, (iy + 9)
	inc	b
	dec	b
	jr	z, .finish
.loop:
	add	hl, hl
	ex	de, hl
	adc	hl, hl
	ex	de, hl
	rl	c
	rla
	djnz	.loop
.finish:
	ld	b, a
	pop	iy
	pop	af
	ret
