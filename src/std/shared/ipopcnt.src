	assume	adl=1

	public	__ipopcnt
__ipopcnt:
	push	bc
	push	hl
	xor	a, a
	ld	c, a
	ld	b, 3
.loop:
	add	hl, hl
	adc	a, c
	add	hl, hl
	adc	a, c
	add	hl, hl
	adc	a, c
	add	hl, hl
	adc	a, c
	add	hl, hl
	adc	a, c
	add	hl, hl
	adc	a, c
	add	hl, hl
	adc	a, c
	add	hl, hl
	adc	a, c
	djnz	.loop
	pop	hl
	pop	bc
	ret
